pipeline {
   agent any

   stages {
      stage('Venv') {
        steps {
		sh 'pwd'
		python --version
          sh 'venv/Scripts/activate'
		  sh 'pip install -r requirements.txt'
		  echo 'venv activated'
        }
      }
      stage('Go') {
        steps{      
         sh 'robot Tests/tests.robot'
        }
		}
  }
} 