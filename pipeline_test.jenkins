pipeline {
   agent any

   stages {
      stage('Venv') {
        steps {
			sh 'python -m venv venv'
          sh 'venv/Scripts/activate'
		  sh 'pip install -r requirements.txt'
		  echo 'venv activated'
        }
      }
      stage('Go') {
        steps{      
         sh 'robot Tests/tests.robot'
        }
		}
  }
} 